    <script src="/frontoffice/lib/owlcarousel/owl.carousel.min.js"></script>

    <!-- Contact Javascript File -->
    <script src="/frontoffice/mail/jqBootstrapValidation.min.js"></script>
    <script src="/frontoffice/mail/contact.js"></script>

